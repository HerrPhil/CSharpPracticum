@using DealerOrders.ViewModels
@model FileUploadViewModel

@{
    Layout = "~/Views/Shared/_CommonLayout.cshtml";
}

@* optional header section not needed *@

@section TitleSection{
    Bulk Upload
}

@section ContentBody{
    <div>
        <a href="/Order/Index">Back</a>
        <form action="/BulkUpload/Upload" method="post" enctype="multipart/form-data">
            Select File: <input type="file" name="fileUpload" value="" />
            <input type="submit" name="name" value="Upload" />
        </form>
    </div>
}